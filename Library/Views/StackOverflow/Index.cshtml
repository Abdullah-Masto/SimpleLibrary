@model List<SimpleLibrary.Services.Question>

@{
    ViewData["Title"] = "Stack Overflow Questions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container mt-4">
    <h1 class="mb-4">@ViewData["Title"]</h1>
    <div class="list-group">
        @foreach (var question in Model)
        {
            <a href="@Url.Action("Details", "StackOverflow", new { id = question.question_Id })" class="list-group-item list-group-item-action">
                @question.Title
            </a>
        }
    </div>
</div>
